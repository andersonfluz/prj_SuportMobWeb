            <p>@Html.DisplayFor(model => model.Observacao)</p>
            <p>@Html.DisplayNameFor(model => model.DataHoraAbertura)</p>
            <p>@Html.DisplayFor(model => model.DataHoraAbertura)</p>
            <p>@Html.DisplayNameFor(model => model.StatusChamado)</p>
            <p>@Html.DisplayFor(model => model.StatusChamado)</p>
            <p>@Html.DisplayNameFor(model => model.ObsevacaoInterna)</p>
            <p>@Html.DisplayFor(model => model.ObsevacaoInterna)</p>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
        </div>
    </div>
</div>
